@model cd_c_weddingPlanner.Models.LoginUser

<form action="/Login" method="post" class="form-group">
    <div class="card m-1 p-3">
        <div >
            <span class="mtb-2 text-danger" asp-validation-for="LoginEmail" ></span>
        </div>
        <label asp-for="LoginEmail"></label>
        <input asp-for="LoginEmail" class="form-control">

        <div>
            <span class="mtb-2 text-danger" asp-validation-for="LoginPassword" ></span>
        </div>
        <label asp-for="LoginPassword"></label>
        <input asp-for="LoginPassword" class="form-control">
    </div>
    <div>
        <input type="submit" value="Login User" class="btn btn-outline-dark mt-3">
    </div>
</form>